<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.com.group_portfolio_hw_team_black_mirror">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.Light.NoActionBar">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".SplashPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Biography"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Marly_Biography"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Khaing_Biography"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Gregg_Biography"
            android:parentActivityName=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        android:theme="@style/Gregg_app_theme"


    </application>

</manifest>